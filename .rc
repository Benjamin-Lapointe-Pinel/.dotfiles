# If not running interactively, don't do anything
[[ $- != *i* ]] && return

source $HOME/.env
source $HOME/.aliases
source $HOME/.base16_theme

[[ -f $HOME/.rc_custom ]] && source $HOME/.rc_custom

if [[ $(tput cols) -ge 100 ]]
then
	bash $HOME/.color-scripts/pacman
elif [[ $(tput cols) -ge 80 ]]
then
	bash $HOME/.color-scripts/space-invaders
elif command -v neofetch &> /dev/null
then
	neofetch
fi
