# If not running interactively, don't do anything
[[ $- != *i* ]] && return
#
# disable suspend output
stty susp undef

source $HOME/.env
source $HOME/.aliases
source $HOME/.base16_theme

[[ -f $HOME/.rc_custom ]] && source $HOME/.rc_custom

[ -x "$(command -v tmux)" ] && [ -z "${TMUX}" ] && tmux new -As0 && exit $?

